package excise;

public class Trans {
	public int fromStrToInt(String str){
		int neg;
		int i=0;
		if(str.charAt(i)=='-'){
			neg=1;
		}
		if(str.charAt(i)=='+'){
			neg=0;
		}
		i++;
		int num=0;
		while(i<str.length()){
			int temp=str.charAt(i)-'0';
			num=num*10+temp;
			i++;
		}
		return num;
	}
	pub
}
